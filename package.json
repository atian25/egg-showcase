{
  "name": "showcase",
  "version": "1.0.0",
  "description": "showcase for egg issue",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.21.1",
    "egg-instrument": "^2.0.0",
    "egg-scripts": "^2.11.0",
    "egg-view-nunjucks": "^2.2.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.22.2",
    "eslint": "^5.16.0",
    "eslint-config-egg": "^7.3.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": "bin/index.js",
  "pkg": {
    "assets": [ "app/view/**/*", "./package.json" ],
    "scripts": [ "app/**/*.js", "app/*.js", "config/*.js", "config/**/*.js", "app.js" ]
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-showcase",
    "stop": "egg-scripts stop --title=egg-server-showcase",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:atian25/egg-showcase.git"
  },
  "author": "TZ <atian25@qq.com>",
  "license": "MIT"
}
